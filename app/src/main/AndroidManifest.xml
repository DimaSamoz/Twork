<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="uk.ac.cam.grp_proj.mike.twork_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="uk.ac.cam.grp_proj.mike.twork_app.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="uk.ac.cam.grp_proj.mike.twork_app.MainActivity"
            android:label="@string/title_activity_home_screen"
            android:theme="@style/AppTheme"/>

        <service android:name="uk.ac.cam.grp_proj.mike.twork_service.CompService" android:enabled="true"/>
    </application>

</manifest>
